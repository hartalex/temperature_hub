{
  "name": "temperature_hub",
  "version": "1.0.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node scripts/test.js --env=jsdom",
    "clean": "rimraf build",
    "build": "NODE_ENV=production npm run clean && babel src/server -d build/server && webpack",
    "lint": "eslint src/**",
    "start": "npm run build && node build/server/server.js",
    "version": "node -v",
    "coverage": "nyc report --reporter=text-lcov > lcov.info",
    "cleanOldContainers": "node scripts/cleanOldContainers.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hartalex/temperature_hub.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hartalex/temperature_hub/issues"
  },
  "homepage": "https://github.com/hartalex/temperature_hub#readme",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "body-parser": "^1.16.1",
    "cors": "^2.8.1",
    "ejs": "^2.5.7",
    "es6-promise": "^4.0.5",
    "express": "^4.15.5",
    "express-cache-headers": "^0.1.3",
    "express-force-ssl": "^0.3.2",
    "express-winston": "^2.4.0",
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^3.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.12.0",
    "react-google-charts": "^1.5.3",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "request": "^2.83.0",
    "socket.io-client": "^2.2.0",
    "whatwg-fetch": "^2.0.3",
    "winston": "2.4.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.5",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-minify-mangle-names": "^0.5.0",
    "cross-env": "^5.1.4",
    "css-loader": "^3.0.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsdoc": "^15.9.10",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.8.0",
    "mockdate": "^2.0.3",
    "nyc": "^14.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-test-renderer": "^16.8.6",
    "redux-devtools": "^3.5.0",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "simple-mock": "^0.8.0",
    "style-loader": "^0.20.3",
    "supertest": "^4.0.2",
    "url-loader": "^1.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/client/**/*.{js,jsx,mjs}",
      "src/server/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
